{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "templateUrl": {
            "type": "string",
            "metadata": {
                "description": "URL where templates are stored."
            }
        },
        "templateSasToken": {
            "type": "string",
            "metadata": {
                "description": "SAS token used for referencing templates."
            }
        }
    },
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "connectionDeployment",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateUrl'), 'Connection.json', parameters('templateSasToken'))]",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "logicAppDeployment",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateUrl'), 'LogicApp.json', parameters('templateSasToken'))]",
                    "contentVersion": "1.0.0.0"
                }
            },
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'connectionDeployment')]"
            ]
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "name": "keyVaultDeployment",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateUrl'), 'KeyVault.json', parameters('templateSasToken'))]",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ],
    "outputs": {
    },
    "functions": [
    ]
}